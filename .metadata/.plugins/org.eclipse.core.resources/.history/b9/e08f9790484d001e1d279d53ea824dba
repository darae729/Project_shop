	public void insert(Gallery gallery) {
		SqlSession sqlSession=mybatisConfig.getSqlSession();
		
		int result = sqlSession.insert("Gallery.insert", gallery);
		if(result==0) {
			//개발자가 일부러 관련있는 에러를 일으키자
		}
	}

